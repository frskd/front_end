{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-react"],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "double", "jsx-double"],
        "interface-name": [false],
        "jsx-no-multiline-js": [false],
        "semicolon": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "no-console": false,
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
